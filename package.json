{
  "name": "dex-com",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "serve": "vue-cli-service serve --host 0.0.0.0",
    "build:dev": "vue-cli-service build --mode development",
    "lint": "vue-cli-service lint --fix",
    "start": "npm run serve",
    "analyzer": "use_analyzer=true npm run serve",
    "build:prod": "vue-cli-service build --mode production",
    "serve:prod": "vue-cli-service serve --host 0.0.0.0 --mode production"
  },
  "dependencies": {
    "9cf-chart": "^1.0.20",
    "axios": "^0.21.0",
    "bignumber.js": "^9.0.1",
    "compression-webpack-plugin": "^5.0.2",
    "core-js": "^3.6.5",
    "decimal.js": "^10.2.1",
    "echarts": "^4.9.0",
    "element-ui": "^2.14.1",
    "fastclick": "^1.0.6",
    "sass": "^1.29.0",
    "sass-loader": "^10.1.0",
    "scss": "^0.2.4",
    "vue": "^2.6.11",
    "vue-router": "^3.4.9",
    "vuex": "^3.5.1",
    "web3": "^1.3.0",
    "web3-utils": "^1.3.0"
  },
  "devDependencies": {
    "@vue/cli-plugin-babel": "~4.5.0",
    "@vue/cli-plugin-eslint": "~4.5.0",
    "@vue/cli-service": "~4.5.0",
    "babel-eslint": "^10.1.0",
    "babel-plugin-component": "^1.1.1",
    "cross-env": "^7.0.3",
    "eslint": "^6.7.2",
    "eslint-plugin-vue": "^6.2.2",
    "terser-webpack-plugin": "^4.2.3",
    "vue-i18n": "^8.22.2",
    "vue-template-compiler": "^2.6.11"
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true
    },
    "extends": [
      "plugin:vue/essential"
    ],
    "parserOptions": {
      "parser": "babel-eslint"
    },
    "rules": {
      "generator-star-spacing": "off",
      "no-tabs": "off",
      "no-unused-vars": "off",
      "no-console": "off",
      "no-irregular-whitespace": "off",
      "no-debugger": "off",
      "eqeqeq": "off"
    }
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not dead"
  ]
}
